{
    "name": "awcodes/postal-codes",
    "description": "This is my package postal-codes",
    "keywords": [
        "awcodes",
        "laravel",
        "postal-codes"
    ],
    "homepage": "https://github.com/awcodes/postal-codes",
    "license": "MIT",
    "authors": [
        {
            "name": "awcodes",
            "email": "awcodes1@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "spatie/laravel-package-tools": "^1.14.0",
        "illuminate/contracts": "^10.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.9",
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "Awcodes\\PostalCodes\\": "src/",
            "Awcodes\\PostalCodes\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Awcodes\\PostalCodes\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Awcodes\\PostalCodes\\PostalCodesServiceProvider"
            ],
            "aliases": {
                "PostalCodes": "Awcodes\\PostalCodes\\Facades\\PostalCodes"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}